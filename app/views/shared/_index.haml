.ui-widget-content{ :id => objects.first.class.list_tag }
  = link_to_close_window objects.first.class
  = will_paginate objects rescue nil
  = render :partial => objects.first.class.new_partial, :object => objects.first.class.new unless objects.first.class == Contact
  = render :partial => objects.first.class.edit_partial, :collection => objects
  = draggable_element objects.first.class.list_tag
