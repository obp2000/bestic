%div{ :id => @object.class.name.tableize }
  %h2= index_page_title_for @objects.first.class 
  = render :partial => 'orders/navigation'
  %table{:summary => @page_title}
    %caption
      = will_paginate @objects
    %tr
      %th{:scope => "col"}= Order.id_rus
      %th{:scope => "col"}= Order.status_header_rus
      %th{:scope => "col"}= Order.total_rus
      %th{:scope => "col"}= Order.count_rus
      %th{:scope => "col"}= Order.created_at_rus
      %th{:scope => "col"}= Order.updated_at_rus
      %th{:scope => "col"}
    = render :partial => 'orders/order', :collection => @objects
  #order_details
  #item_content